{
	"name": "yabx/rest-bundle",
	"type": "symfony-bundle",
	"minimum-stability": "stable",
	"description": "REST for Symfony 7.0",
	"license": "MIT",
	"require": {
		"php": "^8.3",
		"doctrine/orm": "^2.15",
		"symfony/framework-bundle": "^6.4|^7.0",
		"symfony/http-kernel": "^6.4|^7.0",
		"symfony/serializer": "^6.4|^7.0",
		"symfony/validator": "^6.4|^7.0"
	},
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Yabx\\RestBundle\\": "src/"
		}
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "6.*.*|7.*.*"
		}
	}
}
